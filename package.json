{
  "name": "spawnee",
  "version": "1.3.0",
  "description": "Spawn and orchestrate Cursor Cloud Agents from task templates",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "spawnee": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "lint": "eslint src/**/*.ts",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist",
    "release:patch": "npm version patch && npm publish && git push && git push --tags",
    "release:minor": "npm version minor && npm publish && git push && git push --tags",
    "release:major": "npm version major && npm publish && git push && git push --tags",
    "npm:key": "npm config set //registry.npmjs.org/:_authToken="
  },
  "dependencies": {
    "@types/inquirer": "^9.0.9",
    "axios": "^1.7.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "inquirer": "^13.2.0",
    "ora": "^8.1.0",
    "yaml": "^2.5.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "cursor",
    "ai",
    "agents",
    "automation",
    "orchestration",
    "spawnee",
    "task-runner",
    "parallel"
  ],
  "author": "spencech",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spencech/spawnee.git"
  },
  "bugs": {
    "url": "https://github.com/spencech/spawnee/issues"
  },
  "homepage": "https://github.com/spencech/spawnee#readme"
}
